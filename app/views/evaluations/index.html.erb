<p id="notice"><%= notice %></p>

<h1>Evaluations</h1>

<table>
  <thead>
    <tr>
      <th>Score</th>
      <th>Comment</th>
      <th>Project</th>
      <th>Team</th>
      <th>User</th>
      <th>Team member</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @evaluations.each do |evaluation| %>
      <tr>
        <td><%= evaluation.score %></td>
        <td><%= evaluation.comment %></td>
        <td><%= evaluation.project.name %></td>
        <td><%= evaluation.team.name %></td>
        <td><%= evaluation.user.email %></td>
        <td><%= evaluation.team_member.user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Back', course_path(id: params[:course_id]) %>
