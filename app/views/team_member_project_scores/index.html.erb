<p id="notice"><%= notice %></p>

<h1>Team Member Project Scores</h1>

<table>
  <thead>
    <tr>
      <th>Score</th>
      <th>Team member</th>
      <th>Project team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @team_member_project_scores.each do |team_member_project_score| %>
      <tr>
        <td><%= team_member_project_score.score %></td>
        <td><%= team_member_project_score.team_member_id %></td>
        <td><%= team_member_project_score.project_team_id %></td>
        <td><%= link_to 'Show', team_member_project_score %></td>
        <td><%= link_to 'Edit', edit_team_member_project_score_path(team_member_project_score) %></td>
        <td><%= link_to 'Destroy', team_member_project_score, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Team Member Project Score', new_team_member_project_score_path %>
